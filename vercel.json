{
  "rewrites": [
    {
      "source": "/",
      "destination": "/web_dashboard/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/web_dashboard/$1"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}
