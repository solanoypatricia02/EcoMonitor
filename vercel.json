{
  "version": 2,
  "name": "ecomonitor",
  "builds": [
    {
      "src": "web_dashboard/**",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/web_dashboard/index.html"
    },
    {
      "src": "/(.*)",
      "dest": "/web_dashboard/$1"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/web_dashboard/$1"
    }
  ],
  "headers": [
    {
      "source": "/sw.js",
      "headers": [
        {
          "key": "Service-Worker-Allowed",
          "value": "/"
        }
      ]
    }
  ]
}
